{{#if successMessage}}
  <script>
    document.querySelector('script[src="/bundle.js"]').onload = ()=>{
        M.toast({
            html: "<i class='material-icons'>donde</i> {{ successMessage }}",
            classes: 'card-panel accent-4 white-text',
            displayLenght: 2000
        })
    }
  </script>
{{/if}}
{{#if infoMessage}}
  <script>
    document.querySelector('script[src="/bundle.js"]').onload = ()=>{
      M.toast({html: '{{infoMessage}}', classes: 'info', displayLength: 2000 })
    }
  </script>
{{/if}}
{{#if errorMessage}}
  <script>
    document.querySelector('script[src="/bundle.js"]').onload = ()=>{
      M.toast({html: '{{errorMessage}}', classes: 'error', displayLength: 2000})
    }
  </script>
{{/if}}
{{#if passportError}}
  <script>
    document.querySelector('script[src="/bundle.js"]').onload = ()=>{
      M.toast({html: '{{passportError}}', classes: 'error', displayLength: 2000})
    }
  </script>
{{/if}}