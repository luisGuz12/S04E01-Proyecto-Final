{
    "parser": "@babel/eslint-parser",
    "plugins": ["prettier"],
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": ["airbnb-base", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["error", "single"],
        "no-console":"off",
        "prettier/prettier": ["error"]
    },
    "globals": {
        "_rootdir": "readonly",
        "__rootdir": "readonly",
        "M": "readonly"
    },
    "settings": {
        "import/resolver": { "babel-module": {} }
    }
}