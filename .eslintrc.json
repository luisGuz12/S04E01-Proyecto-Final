{
    "parser": "@babel/eslint-parser",
    "plugins": ["prettier"],
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console":"off",
        "prettier/prettier": ["error"]
    },
    "globals": {
        "_rootdir": "readonly",
        "__rootdir": "readonly",
        "M": "readonly"
    }
}