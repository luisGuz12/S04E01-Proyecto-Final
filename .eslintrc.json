{
    "parser": "@babel/eslint-parser",
    "plugins": ["prettier"],
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": ["airbnb-base", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console":"off",
        "prettier/prettier": ["error"],
        "no-underscore-dangle": ["error", { "allow": ["_id"] }]
    },
    "globals": {
        "_rootdir": "readonly",
        "__rootdir": "readonly",
        "M": "readonly"
    }
}