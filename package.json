{
"name": "s04e01-proyecto-final",
"version": "0.0.0",
"private": true,
"customizations": {
  "vscode": {
    "extensions": [
      "mongodb.mongodb-vscode",
				"PKief.material-icon-theme",
				"ritwickdey.LiveServer",
				"meganrogge.template-string-converter",
				"wayou.vscode-todo-highlight",
				"rangav.vscode-thunder-client",
				"eamodio.gitlens",
				"usernamehw.errorlens",
				"esbenp.prettier-vscode",
				"christian-kohler.path-intellisense",
				"dbaeumer.vscode-eslint"
    ]
  }
},
"scripts": {
  "start": "node ./dist/bin/www",
    "dev": "NODE_ENV=development DEBUG=s04e01-proyecto-final:server babel-watch ./server/bin/www",
    "devc": "NODE_ENV=development webpack serve --config ./webpack.dev.config.js --mode development",
    "devb": "DEBUG=s04e01-proyecto-final:server && babel-watch ./server/bin/www",
    "cleans": "rm -rf ./dist",
    "cleanc": "rm -f ./public/bundle.js ./public/styles/app.css",
    "b:s": "npm run cleans && babel ./server --out-dir dist --copy-files",
    "b:c": "npm run cleanc && webpack --config ./webpack.config.js --mode production",
    "build": "npm run b:c && npm run b:s",
    "clean:logs": "rm -rf ./server/logs/*.log"
},
"dependencies": {
  "cookie-parser": "~1.4.4",
  "debug": "~2.6.9",
  "express": "~4.16.1",
  "hbs": "~4.0.4",
  "http-errors": "~1.6.3",
  "materialize-css": "^1.0.0-rc.2",
  "morgan": "~1.9.1",
  "webpack-dev-middleware": "^6.0.2",
  "webpack-hot-middleware": "^2.25.3",
  "winston": "3.11.0",
  "webpack": "^5.76.1"
},
"devDependencies": {
  "@babel/cli": "^7.22.15",
  "@babel/core": "^7.22.17",
  "@babel/eslint-parser": "^7.22.15",
  "@babel/node": "^7.22.15",
  "@babel/preset-env": "^7.22.15",
  "babel-loader": "^9.1.3",
  "babel-watch": "^7.8.1",
  "core-js": "^3.32.2",
  "css-loader": "^6.8.1",
  "eslint": "^8.50.0",
  "eslint-config-airbnb-base": "^15.0.0",
  "eslint-config-prettier": "^9.0.0",
  "eslint-plugin-import": "^2.28.1",
  "eslint-plugin-prettier": "^5.0.0",
  "eslint-webpack-plugin": "^4.0.1",
  "mini-css-extract-plugin": "^2.7.6",
  "webpack-cli": "^5.0.1",
  "webpack-dev-server": "^4.11.1"
}
}